    "instant_5_e18_bz_p_uj_hv5z_mtt_DzgLKC": {
      "type": "instant-5E18BzPUjHV5zMTT",
      "name": "Design Section",
      "settings": {
        "text_ns0RuTYPCPx1ogJw": "<p>How it works...</p>",
        "text_Dg8CjI0j7kq78hBQ": "<p>Cutting-Edge Design</p>",
        "text_nOJfdjGunhKuk5wo": "<p>After immense input from our real customers, we've created what we believe to be the best thong for active girls on the market.</p>",
        "text_v774Bb9K3QQspPj9": "<p>1</p>",
        "text_BShyI1KAhv59SldF": "<p>Camel Toe Proof <strong>Technology</strong></p>",
        "text_BUyqaN4TrQBhRYHH": "<p>2</p>",
        "text_Qnw5ojpXcnPiN2Vm": "<p><strong>Premium</strong> Moisture Wicking Fabric</p>",
        "text_QY0ueHp6YnB5Zl4f": "<p>3</p>",
        "text_LGfdGnvEjnDG2Jbm": "<p>Wide <strong>Comfy</strong> Gusset</p>",
        "text_QY0ueHp6YnB5Zl4f2": "<p>4</p>",
        "text_LGfdGnvEjnDG2Jbm2": "<p>Seamless</p>",
        "text_M0qzzoJsFcPr4sow": "<p>Olora: the  invisble thong that let’s you focus on your day, not your underwear.</p>",
