window.addEventListener('load', () => {
  const values = [];
  const fieldSets = document.querySelectorAll("fieldset");

  function syncSelectToValues() {
    const select = document.querySelector("[name=id]");

    for (let i = 0; i < select.options.length; i++) {
      const opt = select.options[i];
      const cleanedText = opt.text.split(" - ")[0];
      const splitText = cleanedText.split(" / ");

      const copy1 = [...splitText].sort();
      const copy2 = [...values].sort();
      
      if (JSON.stringify(copy1) === JSON.stringify(copy2)) {
        select.selectedIndex = i;
        select.dispatchEvent(new Event("change"));
      }
    }
  }

  function clickDefaultOptions() {
    fieldSets.forEach((fieldSet, fieldsetIndex) => {
      const defaultButton = fieldSet.querySelector("label[data-default='true']") || fieldSet.querySelector("label:first-of-type");
      if (defaultButton) {
        const btnVal = defaultButton.getAttribute("title");
        values[fieldsetIndex] = btnVal;
        defaultButton.click();
      }
    });
    setTimeout(() => syncSelectToValues(), 1);
  }

  fieldSets.forEach((fieldSet, fieldsetIndex) => {
    const buttons = fieldSet.querySelectorAll("label");

    buttons.forEach((btn) => {
      const btnVal = btn.getAttribute("title");

      btn.addEventListener("click", () => {
        values[fieldsetIndex] = btnVal;
        setTimeout(() => syncSelectToValues(), 1);
      });
    });
  });

  // Call this function to auto-select default options
  clickDefaultOptions();

  console.log('All assets are loaded');
});


    

