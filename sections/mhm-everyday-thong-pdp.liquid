getImageForDropdown(index) {
    const selectedColor = this.selectedOptions[index]?.color;
    const selectedStyle = this.selectedOptions[index]?.style;
    let selectedImage = this.defaultImages[index % this.defaultImages.length];

    if (selectedColor && selectedStyle && this.imageMap[selectedColor]?.[selectedStyle]) {
        selectedImage = this.imageMap[selectedColor][selectedStyle];
    }

    // Update primary image
   const observer = new MutationObserver(() => {
    const updateImage = (imgDivId, includeSrcSet = false) => {
        const targetImgDiv = document.getElementById(imgDivId);
        if (targetImgDiv) {
            const targetImage = targetImgDiv.querySelector('img');
            if (targetImage) {
                const sliderImage = this.sliderImages[selectedColor]?.[selectedStyle];
                const newSrc = sliderImage || selectedImage;

                ['src', 'data-src', 'data-srcset', 'srcset', 'base-src'].forEach(attr => {
                    targetImage.setAttribute(
                        attr,
                        newSrc + (includeSrcSet && attr.includes('srcset')
                        ? `&width=768 767w, ${newSrc}&width=1024 1023w, ${newSrc}&width=1920 1440w`
                        : '')
                    );
                });
            }
        }
    };

    // Update both images
    updateImage('small-img');
    updateImage('BYwrkRfn3prdGKcU', true);  // Includes `srcset` logic
});

// Observe DOM changes to ensure updates happen dynamically
observer.observe(document.body, { childList: true, subtree: true });



    return selectedImage;
}

  }));
