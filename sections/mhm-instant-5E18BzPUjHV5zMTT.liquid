}

.product-desc-container {
    display: flex;
    justify-content: center;
    align-items: start;
    gap: 48.9px;
    margin-top: 60px;
  width:100%;
}
.product-img {
    border-radius: 20px;
     flex:1;
}
.product-img img {
    width: 100%;
    height: auto;
    background-size: cover;
    
}
.single-desc-container {
    display: flex;
    gap: 28px;
    padding-bottom: 6rem;
    margin-top: 1rem;
}
    .desc-container{
     flex:1; 
    }
.desc-content span {
    color: #1B0E04;
    font-size: 22px;
    font-weight: 500;
}
.desc-content p {
    margin-top: 10px;
    font-size: 16px;
    font-weight: 400;
    line-height: 26px;
    color: #29433B;
}

.nmbr-container {
    position: relative;
}
.nmbr-container .nmbr {
    background-color: #f2d1cc;
    width: 90px;
    height: 90px;
    border-radius: 100px;
    border: 2px solid #000000 !important;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 38px;
    font-weight: 500;
}
.nmbr-container::after {
    content: "";
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    width: 3px;
    background: repeating-linear-gradient(
        to bottom,
        #D9D9D9 0,
        #D9D9D9 10px,
        /* Adjust circle size */ transparent 10px,
        transparent 20px /* Adjust spacing between circles */
    );
    height: calc(100% - 1rem);
    margin-top: 1rem;
    /* Adjust the position and size to fit your container */
}
.single-desc-container:last-child > .nmbr-container::after {
    display: none;
}
.single-desc-container:first-child {
    margin-top: 0;
}

.pink-dot {
    position: absolute;
    top: calc(100% + 6rem);
    z-index: 2;
    /* right: 0; */
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
    border-radius: 50%;
    width: 0.5rem;
    height: 0.5rem;
    background-color: #c2818a;
}
.bottom-dot {
    top: calc(0% + 94px);
}

/* Ensure Mobile Layout doesn't conflict with Desktop */

@media (max-width: 1040px) {
   .product-img img {
