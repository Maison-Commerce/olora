            <script>
              // Initialize the countdown number
              let number = 65;

              // Function to decrease the number and reset if it reaches zero
              function countdown() {
                  // Get the countdown element
                  let countdownElement = document.getElementById("countdownss");

                  // Add the fade class to apply the fade out effect
                  countdownElement.classList.add("fade");

                  // After the fade-out transition, update the number and fade in
                  setTimeout(() => {
                      // Decrease the number by 1
                      number--;

                      // Update the countdown display
                      countdownElement.innerText = number;

                      // Remove the fade class to apply the fade in effect
                      countdownElement.classList.remove("fade");

                      // Check if the number has reached zero
                      if (number <= 0) {
                          // Reset the number to 65
                          number = 65;
                          countdownElement.innerText = number;
                      }
                  }, 500); // Match the duration of the fade-out transition
              }

              // Set an interval to run the countdown function every 5 seconds (5000 milliseconds)
              setInterval(countdown, 5000);
            </script>
