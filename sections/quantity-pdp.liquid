        <script>
              document.addEventListener('DOMContentLoaded', function() {
                  const quantityInput = document.querySelector('.instant-quantity-input');
                  const priceElement = document.querySelector('[data-instant-dynamic-content-source="PRICE"]');
                  const additionalPriceElement = document.querySelector('.iVfXZ3v3SFioAhcW5');
                  const compareAtPriceElement = document.querySelector('[data-instant-dynamic-content-source="COMPARE_AT"]');
                  const savedAmountElement = document.querySelector('[data-instant-dynamic-content-source="SAVED_AMOUNT"]');

                  // Function to remove currency symbols and commas, leaving only numbers and decimal points
              function parsePrice(priceString) {
              console.log("Original price string:", priceString); // Debug: show original string

              // Step 1: Remove currency symbols and punctuation immediately following them
              let cleanedString = priceString.replace(/^[^\d]+[.,]?/, "");
              console.log("After removing currency symbols and initial punctuation:", cleanedString); // Debug

              // Step 2: Check for both '.' and ',' in the string
              if (cleanedString.includes(".") && cleanedString.includes(",")) {
                  // Case: Both '.' and ',' are present, assume '.' is thousands separator and ',' is decimal
                  cleanedString = cleanedString.replace(/\./g, "").replace(",", ".");
                  console.log("Both ',' and '.' present - treated '.' as thousands separator:", cleanedString); // Debug
              } else if (cleanedString.includes(",")) {
                  // Case: Only ',' is present
                  // Check if ',' is followed by exactly two digits at the end of the string
                  if (/,\d{2}$/.test(cleanedString)) {
                      // If so, treat ',' as a decimal separator
                      cleanedString = cleanedString.replace(",", ".");
                      console.log("Only ',' present and followed by two digits - treated as decimal separator:", cleanedString); // Debug
                  } else {
                      // Otherwise, treat ',' as a thousands separator
                      cleanedString = cleanedString.replace(/,/g, "");
                      console.log("Only ',' present but not followed by two digits - treated as thousands separator:", cleanedString); // Debug
                  }
              } else {
                  // Case: Only '.' present or no separator, assume standard format
                  cleanedString = cleanedString.replace(/,/g, "");
                  console.log("Only '.' present or no separator - treated ',' as thousands separator:", cleanedString); // Debug
              }

              // Step 3: If the cleaned string starts with a decimal point, remove it
              if (cleanedString.startsWith('.')) {
                  cleanedString = cleanedString.slice(1);
                  console.log("Leading decimal point removed:", cleanedString); // Debug
              }

              // Convert to float
              const numericValue = parseFloat(cleanedString);
              console.log("Final parsed numeric value:", numericValue); // Debug: show final parsed value
              return numericValue;
          }




                  // Get the base prices from the page initially
                  const basePrice = parsePrice(priceElement.textContent);

                  let baseCompareAtPrice = 0;
                  if (compareAtPriceElement && compareAtPriceElement.textContent) {
                      baseCompareAtPrice = parsePrice(compareAtPriceElement.textContent);
                  }

                  quantityInput.addEventListener('change', function() {
                      const quantity = parseInt(quantityInput.value);

                      if (!isNaN(basePrice) && quantity > 0) {
                          const newPrice = (basePrice * quantity).toFixed(2);

                          // Format price with appropriate currency symbol
                          priceElement.textContent = new Intl.NumberFormat(undefined, {
                              style: 'currency',
                              currency: Shopify.currency.active,
                              currencyDisplay: 'narrowSymbol'
                          }).format(newPrice);

                          additionalPriceElement.textContent = '(' + new Intl.NumberFormat(undefined, {
                              style: 'currency',
                              currency: Shopify.currency.active,
                              currencyDisplay: 'narrowSymbol'
                          }).format(newPrice) + ')';

                          // Update compare at price if available
                          if (!isNaN(baseCompareAtPrice) && baseCompareAtPrice > basePrice) {
                              const newCompareAtPrice = (baseCompareAtPrice * quantity).toFixed(2);
                              compareAtPriceElement.textContent = new Intl.NumberFormat(undefined, {
                                  style: 'currency',
                                  currency: Shopify.currency.active,
                                  currencyDisplay: 'narrowSymbol'
                              }).format(newCompareAtPrice);

                              // Calculate and update saved amount
                              const savedAmount = ((baseCompareAtPrice - basePrice) * quantity).toFixed(2);
                              savedAmountElement.textContent = 'Save ' + new Intl.NumberFormat(undefined, {
                                  style: 'currency',
                                  currency: Shopify.currency.active,
                                  currencyDisplay: 'narrowSymbol'
                              }).format(savedAmount);
                          } else {
                              savedAmountElement.textContent = 'No savings';
                          }
                      } else {
                          priceElement.textContent = 'Invalid Quantity or Price';
                      }
                  });
              });
        </script>

